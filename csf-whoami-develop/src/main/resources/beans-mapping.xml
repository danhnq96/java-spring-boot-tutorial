<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="userController" class="com.csf.whoami.common.base.bean.SystemBean">
        <property name="name" value="com.csf.whoami.web.controller.UserController"/>
        <property name="model" value="com.csf.whoami.common.domain.UserDomain"/>
        <property name="service" value="com.csf.whoami.backend.service.UserService"/>
        <property name="repository" value="com.csf.whoami.common.base.repository.BaseRepository"/>
    </bean>
</beans>